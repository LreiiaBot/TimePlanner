<Window x:Class="TimePlannerUpdated.WPFGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimePlannerUpdated.WPFGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid DataContext="{StaticResource mvm}">
        <DataGrid ItemsSource="{Binding Lists}" HorizontalAlignment="Left" Height="334" Margin="25,23,0,0" VerticalAlignment="Top" Width="373" AutoGenerateColumns="True">
        </DataGrid>
        <DataGrid ItemsSource="{Binding Reminders}" HorizontalAlignment="Left" Height="162" Margin="414,47,0,0" VerticalAlignment="Top" Width="178" AutoGenerateColumns="True">

        </DataGrid>

    </Grid>
</Window>
